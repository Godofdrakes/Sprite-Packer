<Window x:Class="Sprite_Packer.Window_ExportPreview"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Export Preview" Height="300" Width="300"
        Background="Gray">
    <DockPanel>
        <Menu DockPanel.Dock="Top" MaxHeight="22">
            <MenuItem Header="_Save" VerticalAlignment="Center" />
            <MenuItem Header="_Cancel" VerticalAlignment="Center" />
        </Menu>
        <Image Name="imagePreview" />
    </DockPanel>
</Window>
